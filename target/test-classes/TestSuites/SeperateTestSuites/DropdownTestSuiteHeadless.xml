<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="DropdownTestSuite" verbose="1">

	<test name="DropdownTests-chromeHeadless">
		<parameter name="browser" value="chromeHeadless"></parameter>
		<classes>
			<class name="com.webapp.dropdowntests.DropdownTest" />
		</classes>
	</test>

	<test name="DropdownTests-firefoxHeadless">
		<parameter name="browser" value="firefoxHeadless"></parameter>
		<classes>
			<class name="com.webapp.dropdowntests.DropdownTest" />
		</classes>
	</test>
</suite>
